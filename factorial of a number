public class Main
{
	public static void main(String[] args) {
		System.out.println(fact1(5));
	}
	static int fact(int n){
	    int temp=1;
	    for(int i=2;i<=n;i++){
	        temp=temp*i;
	    }
	    return temp;
	}
	static int fact1(int n){
	    if(n==0){
	        return 1;
	    }
	    return n*fact(n-1);
	}
}
